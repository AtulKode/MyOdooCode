    <!-- Adding CRM Dashboard to the CRM Menu -->
    <menuitem name="App Dashboard" id="approval_dashboard_menu"
              parent="approvals.approvals_menu_root"
              sequence="0" action="action_approval_request_all_dashboard"/>
</odoo>

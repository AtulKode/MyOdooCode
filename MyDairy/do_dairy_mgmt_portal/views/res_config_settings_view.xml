<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_dairy_view" model="ir.ui.view">
        <field name="name">res.config.settings.dairy.website</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="20"/>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@id='website_email_marketing']" position="after">
	    	    <block title="Dairy" id="dairy_signup">
	                <setting string="Dairy Signup">
	                    <field name="dairy_signup"/>
	                </setting>
	            </block>
            </xpath>
        </field>

    </record>
</odoo>
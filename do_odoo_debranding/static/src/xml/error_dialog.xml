<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <t t-name="do_odoo_debranding.ErrorDialog" t-inherit="web.ErrorDialog" t-inherit-mode="extension">
        <xpath expr="//p[@t-if='props.message']" position="replace">
        </xpath>
    </t>
    <t t-name="do_odoo_debranding.SessionExpiredDialog" t-inherit="web.SessionExpiredDialog" t-inherit-mode="extension">
        <xpath expr="//p[hasclass('text-prewrap')]" position="replace">
          <p class="text-prewrap">
            Your session expired. The current page is about to be refreshed.
          </p>
        </xpath>
    </t>
</odoo>